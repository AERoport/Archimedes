<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="trimmer" name="Element Trimmer" schema="schema/trimmer.exsd"/>
   <extension
   		 id="br.org.archimedes.trims.default"
         name="The default trimmer finder"
         point="br.org.archimedes.trims">
         <manager class="br.org.archimedes.trims.TrimManager"/>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="br.org.archimedes.trims.actionSet"
            label="Trim action sets"
            visible="true">
         <action
               class="br.org.archimedes.trims.TrimAction"
               icon="icons/trim.png"
               id="br.org.archimedes.trims.action"
               label="Trim"
               menubarPath="transform/simple1"
               style="push"
               toolbarPath="transform/simple1"
               tooltip="Trims the selected element">
         </action>
      </actionSet>
   </extension>

</plugin>
